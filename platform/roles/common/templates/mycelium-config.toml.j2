# Mycelium configuration
[network]
# Peers to connect to
peers = []

# Private key (will be generated if not specified)
# private_key = ""

# Enable routing
enable_routing = true

# Enable NAT traversal
enable_nat_traversal = true

# API configuration
[api]
# API listen address
listen = "127.0.0.1:8989"

# Enable API
enable = true